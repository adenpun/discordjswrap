{
    "name": "discordjswrap",
    "version": "0.1.0",
    "description": "A wrapper for discord.js",
    "repository": "adenpun/discordjswrap",
    "license": "MIT",
    "sideEffects": false,
    "type": "module",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.cjs"
        }
    },
    "main": "./dist/index.cjs",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "unbuild",
        "prepack": "pnpm run build"
    },
    "devDependencies": {
        "deepmerge": "^4.3.0",
        "discord.js": "^14.7.1",
        "typescript": "^4.9.5",
        "unbuild": "^1.1.1"
    },
    "peerDependencies": {
        "discord.js": "^14.7.1"
    }
}
